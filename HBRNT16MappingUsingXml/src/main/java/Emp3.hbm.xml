<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.mapping.xml.Emp3" table="Emp3">
		<id name="id" column="emp_id"></id>
		<property name="name" column="emp_name" type="string"></property>
		<property name="address" column="emp_address" type="string"></property>
		<property name="phone" column="emp_phone" type="string"></property>
	</class>
</hibernate-mapping>